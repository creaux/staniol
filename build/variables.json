{
    "border-style" : "solid",
    "border-width" : 1,
    "border-color" : "#CCCCCC",
    "text-shadow-x" : 1,
    "text-shadow-y" : 1,
    "text-shadow-blur" : 1,
    "text-shadow-color" : "@black",
    "impression-font-color" : "@white",
    "impression-background-color" : "@gray",
    "button-color" : "@white",
    "button-background-color" : "@red",
    "transparent" : "rgba(0,0,0,0)",
    "white" : "rgb(255,255,255)",
    "black" : "rgb(0,0,0)",
    "green" : "rgb(0, 255, 0)",
    "blue" : "rgb(0, 0, 255)",
    "red" : "rgb(255, 0, 0)",
    "gray" : "multiply(@white, @black)",
    "gray-darker" : "darken(@gray, 25%)",
    "gray-dark" : "darken(@gray, 50%)",
    "gray-light" : "lighten(@gray, 25%)",
    "gray-lighter" : "lighten(@gray, 75%)",
    "gray-lightest" : "lighten(@gray, 95%)",
    "blue-light" : "screen(@blue, @gray-light)",
    "blue-lighter" : "screen(@blue, @gray-lighter)",
    "font-family" : "Arial, Helvetica, sans-serif"
}